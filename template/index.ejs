<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no, minimal-ui" />
  <meta name="apple-mobile-web-app-capable" content="yes" />
  <meta name="apple-mobile-web-app-status-bar-style" content="black" />
  <meta name="format-detection" content="telephone=no" />
  <meta name="format-detection" content="email=no" />
  <meta name="renderer" content="webkit">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
  <meta name="HandheldFriendly" content="True">
  <meta name="mobile-web-app-title" content="<%= htmlWebpackPlugin.options.title %>">
  <meta name="apple-mobile-web-app-title" content="<%= htmlWebpackPlugin.options.title %>">
  <title><%= htmlWebpackPlugin.options.api %></title>
  <%for (var css in htmlWebpackPlugin.options.addLinkCss) {%>
  <link rel="stylesheet" href="<%= htmlWebpackPlugin.options.addLinkCss[css] %>">
  <%}%>
</head>
<body>
<div id="app"></div>
<script>
  /**
   * 声明全局配置项
   * @type {{baseUrl: string, apiPath: string}}
     */
  window.applicationConfig = {
    baseUrl:'127.0.0.1:8676',
    apiPath:'http://bbs.fzbm.com/api/1/'
  }
</script>
</body>
</html>